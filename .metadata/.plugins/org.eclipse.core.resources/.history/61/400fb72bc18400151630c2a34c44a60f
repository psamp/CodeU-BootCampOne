package com.jits.routing;

import java.io.Serializable;

class DistributionCenter implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 9210538976197301513L;
	private String destination;
	private String city;
	
	DistributionCenter(String destination, String city) {
		this.setDestination(destination);
		this.setCity(city);
	}

	String getDestination() {
		return destination;
	}
	
	private void setDestination(String destination) {
		this.destination = destination;
	}
	
	String getCity() {
		return this.city;
	}
	
	private void setCity(String state) {
		this.city = state;
	}

}
