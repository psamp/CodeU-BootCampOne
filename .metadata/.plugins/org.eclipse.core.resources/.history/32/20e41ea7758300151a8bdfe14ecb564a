package com.jits.library;

import java.util.Comparator;

import com.jits.shipping.Parcel;

public class IdAndShippingMethodComparator implements Comparator<Parcel> {

	@Override
	public int compare(Parcel first, Parcel second) {
		int rtn = -3;
		
		if(first.getId() == second.getId() & first.getShippingType().equals(second.getShippingType())) {
			rtn = 0;
			
		} else if(first.getId() > second.getId()){
			rtn = 1;
			
		} else {
			rtn = -1;
			
		}
		
		return rtn;
	}

}
