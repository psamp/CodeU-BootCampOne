package com.jits.report;

import java.util.ArrayList;
import java.util.Collections;
import java.util.TreeSet;

import com.jits.shipping.JitsException;
import com.jits.shipping.Parcel;
import com.jits.shipping.Warehouse;

public class Report {
	
	static String getParcels(ArrayList<Parcel> p){
		String report = "";
		for (Parcel parcel : p) {
			report += parcel.toString() + "\n";
		}
		return report;
	}
	
	public static void main(String[] args) throws JitsException {
		
		Warehouse w = new Warehouse();
		ArrayList<Parcel> pkg = new ArrayList<Parcel>();
		
		Parcel a = new Parcel();
		a.setId("7834562390");
		a.setShippingType("RAL");
		
		Parcel b = new Parcel();
		b.setId("9898452354");
		b.setShippingType("AIR");
		
		Parcel c = new Parcel();
		c.setId("9898452354");
		c.setShippingType("GRD");
		
		pkg.add(a);
		pkg.add(b);
		pkg.add(c);
		
		Collections.sort(pkg);
		
		Report.getParcels(pkg);
	}

}
