package com.jits.report;
import java.util.ArrayList;
import java.util.Collections;
import java.util.TreeSet;

import com.jits.library.IdComparator;
import com.jits.shipping.JitsException;
import com.jits.shipping.Parcel;
import com.jits.shipping.Warehouse;

public class Report {
	
	static String getParcels(TreeSet<Parcel> pkg){
		String report = "";
		for (Parcel parcel : pkg) {
			report += parcel.toString() + "\n";
		}
		System.out.println(report);
		return report;
	}
	
	public static void main(String[] args) throws JitsException {
		
		Warehouse w = new Warehouse();
		TreeSet<Parcel> pkg = new TreeSet<Parcel>(new IdComparator());
		
		Parcel a = new Parcel();
		a.setId("7834562390");
		a.setShippingType("RAL");
		
		Parcel b = new Parcel();
		b.setId("9898452354");
		b.setShippingType("AIR");
		
		Parcel c = new Parcel();
		c.setId("9898452354");
		c.setShippingType("GRD");
		
		Parcel d = new Parcel();
		d.setId("9898452354");
		d.setShippingType("GRD");
		
		pkg.add(a);
		pkg.add(b);
		pkg.add(d);
		pkg.add(c);
	}

}
